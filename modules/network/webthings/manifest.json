{
	"include": [
		"$(MODULES)/network/http/manifest.json",
		"$(MODULES)/network/mdns/manifest.json"
	],
	"modules": {
		"~": [
			"$(MODULES)/network/net/net"
		],
		"*": [
			"./webthings",
			"$(MODULES)/network/dns/moddnsparser"
		],
		"net": "$(MODULES)/network/net/netmdns"
	},
	"preload": [
		"net",
		"webthings"
	],
	"platforms": {
		"...": {
			"warning": "Web Thing API support deprecated"
		}
	}
}
