{
	"build":{
		"ESP32_SUBCLASS": "esp32s2",
		"USE_USB": "1",
		"SDKCONFIGPATH": "./sdkconfig"
	},
	"include": [
		"$(MODULES)/drivers/button/manifest.json"
	],
	"modules": {
		"setup/target": "./setup-target"
	},
	"preload": [
		"setup/target"
	],
	"config": {
		"screen": ""
	},
	"x-defines": {
		"i2c": {
			"sda_pin": 4,
			"scl_pin": 5
		},
		"spi": {
			"miso_pin":37,
			"mosi_pin":35,
			"sck_pin": 36
		}
	}
}
