{
	"build": {
		"USE_USB": "1",
		"UF2_VOLUME_NAME": "MODDABLEnRF"
	},
	"include": [
		"$(MODULES)/drivers/button/manifest.json",
		"$(MODULES)/drivers/led/manifest.json"
	],
	"modules": {
		"setup/target": "./setup-target"
	},
	"preload": [
		"setup/target"
	],
	"config": {
		"led1_pin": 11,
		"led2_pin": 12,
		"led3_pin": 13
	},
	"defines": {
		"xdebugger": {
			"tx_pin": 6,
			"rx_pin": 7,
			"baud": "NRF_UART_BAUDRATE_115200"
		},
		"spi": {
			"interface": "3",
			"hz": "8000000",
			"mosi_pin": 10,
			"miso_pin": 9,
			"sck_pin": 8
		},
		"i2c": {
			"interface": "0",
			"scl_pin": 5,
			"sda_pin": 4
		}
	}
}
